VHD="cam_pkg.vhd sim_feed.vhd sim_sink.vhd pipe_head.vhd txt_util.vhd tb_pipe.vhd"

mkdir -p simu

for i in $VHD; do
    ghdl -i --workdir=simu --work=work $i
done

ghdl -m --workdir=simu --work=work tb 

python tiff2dat.py  > sim.dat
./tb --stop-time=10us 
cat sim.out | python dat2tiff.py
eog output.tiff